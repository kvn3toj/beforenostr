{
  "name": "@coomunity/admin-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "test:e2e:full": "playwright test e2e/full-app-verification.spec.ts --headed",
    "test:e2e:full:headless": "playwright test e2e/full-app-verification.spec.ts",
    "test:e2e:full:debug": "playwright test e2e/full-app-verification.spec.ts --debug",
    "clean-vite-cache": "rm -rf node_modules/.vite",
    "clean:backend": "rm -rf dist",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "npx prisma migrate reset",
    "start:backend": "node dist/main.js",
    "start:backend:dev": "tsx watch --no-cache --clear-screen=false --tsconfig tsconfig.backend.json src/main.ts",
    "start:backend:prod": "npm run clean:backend && npm run build:backend && node dist/main.js",
    "start:backend:compiled": "npm run build:backend && cp -r src/generated dist/ && node dist/main.js",
    "build:backend": "tsc -p tsconfig.backend.json"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google/genai": "^0.14.1",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^5.17.1",
    "@mui/x-date-pickers": "^6.20.2",
    "@nestjs/common": "^11.1.1",
    "@nestjs/core": "^11.1.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.1",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^5.22.0",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.71.10",
    "@tanstack/react-query-devtools": "^5.71.10",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^2.30.0",
    "dexie": "^3.2.4",
    "dompurify": "^3.2.6",
    "dotenv": "^16.5.0",
    "i18next": "^23.10.0",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "nostr-tools": "^2.13.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.51.0",
    "react-i18next": "^13.2.2",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.15.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "set-cookie-parser": "^2.7.1",
    "sonner": "^2.0.3",
    "ts-node": "^10.9.2",
    "zod": "^3.22.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@eslint/js": "^9.21.0",
    "@nestjs/testing": "^11.1.1",
    "@playwright/test": "^1.42.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "buffer": "^6.0.3",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "happy-dom": "^17.4.7",
    "jsdom": "^24.0.0",
    "mermaid": "^11.6.0",
    "prettier": "^3.5.3",
    "prisma": "^5.22.0",
    "tsx": "^4.19.4",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^1.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "peerDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  }
}
