{
  "version": 2,
  "buildCommand": "npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "dist",
  "env": {
    "NODE_ENV": "production",
    "NODE_OPTIONS": "--max-old-space-size=4096",
    "VITE_APP_ENV": "production",
    "VITE_BASE_URL": "https://coomunity.com",
    "VITE_API_BASE_URL": "https://god-backend-j4b6.onrender.com",
    "VITE_NETWORK_API_URL": "https://god-backend-j4b6.onrender.com",
    "VITE_ENABLE_MOCK_AUTH": "false",
    "VITE_ENABLE_MOCK_DATA": "false",
    "VITE_MOCK_MODE": "false",
    "VITE_USE_BACKEND": "true",
    "VITE_MOCK_MARKETPLACE": "false",
    "VITE_MOCK_VIDEOS": "false",
    "VITE_MOCK_SOCIAL": "false",
    "VITE_MOCK_WALLET": "false",
    "VITE_MOCK_CHALLENGES": "false",
    "VITE_MOCK_UPLAY": "false",
    "VITE_ENABLE_ANALYTICS": "true",
    "VITE_BETA_TRACKING": "true"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
