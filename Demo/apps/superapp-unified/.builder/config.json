{
  "apiKey": "YOUR_BUILDER_PUBLIC_KEY",
  "organizationId": "Marketplace",
  "devServer": {
    "url": "http://localhost:3001",
    "command": "npm run dev",
    "port": 3001
  },
  "builderServer": {
    "url": "http://localhost:54618",
    "port": 54618
  },
  "framework": "react",
  "typescript": true,
  "outputDir": "src/components/builder",
  "componentsDir": "src/components",
  "pagesDir": "src/pages",
  "modelsDir": "src/models/builder",
  "iframe": {
    "sandbox": "allow-scripts allow-same-origin allow-forms allow-popups allow-pointer-lock allow-popups-to-escape-sandbox",
    "allowFullscreen": true,
    "loading": "lazy"
  },
  "security": {
    "allowCookies": true,
    "allowLocalStorage": true,
    "allowSessionStorage": true,
    "corsPolicy": "permissive"
  },
  "development": {
    "disableCookieWarnings": true,
    "enableConsoleLogs": false,
    "suppressSecurityWarnings": true
  }
} 